{
  "manifest_version": 2,
  "name": "Youtube Alarm Clock",
  "version": "1.0",
  "description": "An alarm that plays a youtube video at specific time.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
	  "alarms",
    "storage",
    "http://*/",
    "https://*/"
  ]
}